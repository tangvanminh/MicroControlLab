#include "fsm_normal.h"

void fsm_nomal_run(){
	switch (status){
	case INIT:
		red_count = 10;
		green_count = 7;
		yellow_count = 3;
		break;
	case RUNNING_RED:
		break;
	case RUNNING_GREEN:
		break;
	case RUNNING_YELLOW:
		break;
	default:
		break;
	}
}

